/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
	// Or(And(Not(sel), a), And(sel, b))
	
	Not(in = sel, out = notSel);
	And(a = a, b = notSel, out = andaNotSel);
	And(a = sel, b = b, out = andbSel);
	Or(a = andaNotSel, b = andbSel, out = out);
	
}